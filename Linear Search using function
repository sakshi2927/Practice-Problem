#include <iostream>
using namespace std;
int linearsearch(int arr[], int sz, int target)
{

    for (int i = 0; i < sz; i++)
    {
        if (arr[i] == target)
        {
            return i;
        }
    }
    return -1;
}
int main()
{
    int arr[] = {34, 29, 45, 23, 44};
    int sz = 5;
    int target = 23;
    cout << linearsearch(arr, sz, target) << endl;
}
