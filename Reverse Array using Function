#include<iostream>
using namespace std;
int rev(int arr[],int size){
 int start=0;int end=size-1;
 while(start<end){
 swap(arr[start],arr[end]);
 start++;end--;
 
}
return 0;
}
int main(){
    int arr[]={2,3,4,5,6};
    int size=5;
    rev(arr,size);
    for(int i=0;i<size;i++){
    cout<<arr[i]<<" ";
}
}
